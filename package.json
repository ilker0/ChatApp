    "wildemitter": "^1.2.0"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "^0.28.9",
    "firebase": "^4.10.1",
    "react": "^15.6.2",
    "react-router": "^3.0.5",
    "react-router-dom": "^4.2.2",
    "reactfire": "^1.0.0",
    "redux": "^3.7.2",
    "stanza.io": "^9.0.2",
